{
  "name": "@mill/client",
  "projectType": "application",
  "sourceRoot": "apps/client/src",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/client"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "@mill/client:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "dev": false,
          "buildTarget": "@mill/client:build:production"
        }
      }
    }
  },
  "tags": []
}
